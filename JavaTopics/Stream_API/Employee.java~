package Stream_API;

public class Employee1 {
    private int id;
    private String name;
    private String department;
    private double salary;
    private int age;

    public Employee1(int id, String name, String department, double salary, int age) {
        this.id = id;
        this.name = name;
        this.department = department;
        this.salary = salary;
        this.age = age;
    }
}
